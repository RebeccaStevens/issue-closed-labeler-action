{
  "name": "issue-closed-labeler-action",
  "version": "0.0.0-development",
  "private": true,
  "description": "Conditionally add labels to issues when closed.",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "homepage": "https://github.com/RebeccaStevens/issue-closed-labeler-action#readme",
  "bugs": {
    "url": "https://github.com/RebeccaStevens/issue-closed-labeler-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RebeccaStevens/issue-closed-labeler-action.git"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Rebecca Stevens",
    "email": "rebecca.stevens@outlook.co.nz"
  },
  "type": "module",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.mjs",
    "check-format": "prettier --list-different \"./**/*.{md,ts,yml}\"",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,ts}\"",
    "cz": "git-cz",
    "format": "prettier --write \"./**/*.{md,ts,yml}\"",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint .",
    "lint:md": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
    "prepare": "husky install",
    "test": "c8 ava"
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3",
    "@octokit/graphql": "^5.0.0",
    "@octokit/types": "^7.2.0",
    "iter-ops": "^1.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.0.3",
    "@rebeccastevens/eslint-config": "^1.2.2",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/node": "^18.0.0",
    "@types/rollup-plugin-auto-external": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.30.0",
    "@vercel/ncc": "^0.34.0",
    "ava": "^4.3.3",
    "c8": "^10.0.0",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cspell": "^6.8.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.2",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.0",
    "markdownlint-cli": "^0.32.2",
    "marked": "^4.0.12",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "semantic-release": "^19.0.5",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.3.0",
    "tslib": "^2.4.0",
    "type-fest": "^3.0.0",
    "typescript": "^4.7.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=16.0.0"
  }
}
